{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PgSqlConnection": "Server=localhost;Port=5432;Database=blogdb;User ID={User ID};Password={Password};Pooling=true;"
  },
  "JWT": {
    "SigningKey": "{SigningKey}",
    "Issuer": "blogapi",
    "Audience": "blogapi",
    "Expiration": 3600
  }
}